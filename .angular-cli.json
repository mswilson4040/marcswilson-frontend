{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "marcswilson"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "images",
        "globals.css",
        ".htaccess"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "globals.scss",
        "../node_modules/jqueryui/jquery-ui.min.css",
        "../node_modules/font-awesome/css/font-awesome.css",
        "../node_modules/jquery.json-viewer/json-viewer/jquery.json-viewer.css",
        "../node_modules/highcharts/css/highcharts.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/jqueryui/jquery-ui.min.js",
        "../node_modules/jquery.json-viewer/json-viewer/jquery.json-viewer.js",
        "../node_modules/highcharts/js/highcharts.js",
        "../node_modules/jquery-ui-touch-punch/jquery.ui.touch-punch.min.js",
        "../node_modules/popper.js/dist/umd/popper.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
